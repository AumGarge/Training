//Find Friday the 13th
from datetime import date
def test(month, year): 
    return str(date(year,month,13).strftime("%A")=='Monday')

month = 11;
year = 2022;            
print("Month No.: ", month, " Year: ", year);
print("Check whether the said month and year contains a Monday 13th.: " + test(month, year));
month = 6;
year = 2022;            
print("\nMonth No.: ", month, " Year: ", year);
print("Check whether the said month and year contains a Monday 13th.: " + test(month, year)); 

//encoded string

input = "Prince000Smith000123"

 

r = input.replace("000", ", ")
print(r)
s = r.split()
print(s)
dict1 = {"first_name": " ", "last_name": " ",  "id" : " "}
for item in s:
    dict1["first_name"] = s[0].replace(",", "")

    dict1["last_name"] = s[1].replace(",", "")

    dict1["id"] = s[2]

